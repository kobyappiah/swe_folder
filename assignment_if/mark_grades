mark = int (input("enter your rating to a value between 0-10"))

grade = ""

if mark >=8 and mark <10:
   grade ="excellent"
elif mark >=5 and mark <8:
   grade = "good"
elif mark >=3 and mark <5:
   grade = "average" 
